


<IfModule mod_rewrite.c>
RewriteEngine On

# Redirect: "builder/[major].[minor][.bugfix]/*" 
# to:       "[major].[minor]/builder/[bugfix]/*"
RewriteRule ^builder/(\d\.\d)(\.(\d))?/(.*)$  $1/builder/$3/$4 [L]

# Redirect: "src/picoquery-[major].[minor][.bugfix-version]-*.js
# to:       "[major].[minor]/src/picoquery-[major].[minor][.bugfix-version]-*.js"
RewriteRule ^src/(picoquery-(\d\.\d)(\.(\d))?-(.*)\.js)  /$2/src/$1 [L]

# Redirect to LATEST version of builder
RewriteRule ^builder/$  /builder/0.2.1/ [L,R=302]





# Redirect for example "picoquery-0.2-B5-b040.js" to "/builder/0.2/build.php?build=5-b040.js"
# RewriteRule ^src/picoquery-([\d\.]+)-B(.*)  http://sensorlog.rosell.dk/ [L]
# $1/builder/build.php?build=$2 [L]

# Redirect "builder/[major-version].[minor-version][.bugfix-version]" to "[version]/builder/index.html"
# RewriteRule ^builder/(\d\.\d)((\.)(\d))?/?$  $1/builder/$4/index.html [L]
# RewriteRule ^builder/([\d\.]+)/?$  $1/builder/index.html [L]


# Redirect "builder/[version]/*" to "[version]/builder/*"
# RewriteRule ^builder/([\d\.]+)/(.*)$  /$1/builder/$2 [L,R=302]




# Redirect for example "picoquery-0.1-4-1-10f.js" to "/builder/build.php?build=0.1-4-1-10f"

# RewriteRule ^picoquery-([\d\.]+-[a-f\d]+-[a-f\d]+-[a-f\d]+)\.js  ../builder/build.php?build=$1 [L]


</IfModule>



